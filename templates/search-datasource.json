{
    "name": "$AZURE_STORAGEACCOUNT_NAME-datasource",
    "description": "ADO data from the storage account",
    "type": "azureblob",
    "credentials": {
        "connectionString": "ResourceId=/subscriptions/$AZURE_SUBSCRIPTIONID/resourceGroups/$AZURE_RESOURCEGROUP/providers/Microsoft.Storage/storageAccounts/$AZURE_STORAGEACCOUNT_NAME;"
    },
    "container": {
        "name": "ado"
    }
}